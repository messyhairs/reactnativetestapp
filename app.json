{
  "expo": {
    "name": "MeToo",
    "slug": "MeToo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/metoo.png",
    "splash": {
      "image": "./assets/metoo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/metoo.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./ggl.json",
      "useNextNotificationsApi": true,
      "package": "com.nathu.MeToo"
    },
    "web": {
      "favicon": "./assets/metoo.png"
    },
    "ios": {
      "bundleIdentifier": "com.nathu.MeToo"
    }
  }
}
